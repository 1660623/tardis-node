{
  "name": "tardis-client",
  "version": "1.2.4",
  "engines": {
    "node": ">=12"
  },
  "description": "Node.js client for tardis.dev - historical tick-level cryptocurrency market data replay API.",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "repository": "tardis-dev/node-client",
  "homepage": "https://github.com/tardis-dev/node-client",
  "scripts": {
    "build": "tsc",
    "precommit": "lint-staged",
    "test": "npm run build && jest --runInBand",
    "prepare": "npm run build",
    "release": "cross-var \"npm run test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish --access=public\""
  },
  "files": [
    "src",
    "dist",
    "example.js"
  ],
  "keywords": [
    "cryptocurrency data feed",
    "market data",
    "api client",
    "crypto markets data replay",
    "historical data",
    "historical cryptocurrency prices",
    "cryptocurrency api"
  ],
  "license": "MPL-2.0",
  "dependencies": {
    "debug": "^4.1.1",
    "fs-extra": "^8.1.0",
    "p-map": "^3.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.5",
    "cross-var": "^1.1.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)?$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "runkitExampleFilename": "example.js"
}
